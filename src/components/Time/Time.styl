@import '../../css/base.styl'

.time {
  width: 100%;
  height: 100px;
  bottom: 0;
  display: flex;
}


.range {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}
.unit {
  border: 1px solid #546076;
  background: no-repeat;
  border-radius: 22px;
  padding: 2px 17px;
  outline: none;
}

.active {
  border: 1px solid #adadad;
  color: #fff;
}