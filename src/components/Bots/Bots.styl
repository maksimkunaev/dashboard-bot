@import '../../css/base.styl'

.bots {
  width: 100%;

  flex-grow: 1;
}
.bot {
  width: 230px;
  height: 209px;
  padding: 2px 0;
  border-radius: 5px;
  overflow: hidden;
  .botInner {
    background: #252d3f;
    height: 100%;
  }
}

.inner {
  width: 100%;

  flex-grow: 1;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}